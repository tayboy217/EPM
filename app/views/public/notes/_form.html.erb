 <%= form_with model: note, local:true do |f| %>
   <div>キーワード</div>
  <%= f.text_field :word , class: "form-control"%>
   <div>英語</div>
  <%= f.text_area :english, size: "5x5", class: "form-control" %>
   <div>日本語</div>
  <%= f.text_area :japanese, size: "5x5", class: "form-control" %>
  <div class="mt-2">
  <%= f.submit '投稿' , class: "btn btn-outline-info mb-2"%>
  </div>
 <% end %>

<h1>Admin::Contacts#index</h1>
<p>Find me in app/views/admin/contacts/index.html.erb</p>

<h2>お問い合わせ一覧</h2>
  <table>
    <thead>
      <tr>
        <th>ユーザー名</th>
        <th>お問い合わせタイトル</th>
        <th>受信日</th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
         <% @contacts.each do |contact| %>
           <tr>
             <td><%= contact.user.name %></td>
             <td><%= contact.title %></td>
             <td><%= contact.created_at.strftime("%Y年%m月%d日 ") %></td>
             <td><%= link_to "返信", edit_admin_contact_path(contact) %></td>
             <td><%= link_to "削除", admin_contact_path(contact), method: :delete, data: { confirm: "削除しますか？" } %></td>
           </tr>
         <% end %>
    </tbody>
  </table>